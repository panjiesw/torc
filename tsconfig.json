{
	"compilerOptions": {
		"target": "es5",
		"module": "es6",
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"experimentalDecorators": true,
		"removeComments": false,
		"charset": "utf8",
		"noImplicitReturns": true,
		"noImplicitThis": true,
		"noUnusedParameters": true,
		"strictNullChecks": true,
		"jsx": "react",
		"baseUrl": ".",
		"paths": {
			"torc/*": [
				"./src/*"
			]
		}
	},
	"exclude": [
		"node_modules"
	]
}
